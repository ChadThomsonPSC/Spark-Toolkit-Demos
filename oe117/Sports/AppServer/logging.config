{
    "logger": {
        "Spark.Core.Util.ABLObjectsReport": {
            "logLevel": "TRACE",
            "filters": [
                "ABL_SUBSTITUTE_FORMAT",
                "ERROR_FORMAT",
                {
                    "name": "TOKEN_FORMAT",
                    "format": "[${t.now}] ${msg.logger} | ${msg.level} - ${msg}"
                },
                {
                    "name": "NAMED_FILE_WRITER",
                    "fileName": "${session.temp-dir}/metrics/ABLObjectsReport/Actions-${t.today}.log",
                    "appendTo": true
                }
            ]
        },
        "Spark.Core.Util.OEMetrics": {
            "logLevel": "TRACE",
            "filters": [
                "ABL_SUBSTITUTE_FORMAT",
                "ERROR_FORMAT",
                {
                    "name": "TOKEN_FORMAT",
                    "format": "[${t.now}] ${msg.logger} | ${msg.level} - ${msg}"
                },
                {
                    "name": "NAMED_FILE_WRITER",
                    "fileName": "${session.temp-dir}/metrics/OEMetrics-${t.today}.log",
                    "appendTo": true
                }
            ]
        }
    }
}